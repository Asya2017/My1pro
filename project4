#include <stdio.h>
#include <stdlib.h>

#include <time.h>

void copy(int *a, int *b, int x) {
    for (int i = 0; i < x; i++) b[i] = a[i];
}

void bub(int *a, int x) {
    for (int i = 0; i < x - 1; i++)
        for (int j = 0; j < x - i - 1; j++)
            if (a[j] > a[j + 1]) {
                int t = a[j];
                a[j] = a[j + 1];
                a[j + 1] = t;
            }
     }
