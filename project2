#include <stdio.h>
int a(int n, int k) {
    int c = 0;
    for (int i = 1; i <= n / 2; i++)
        if (n % i == 0) c++;
    return c == k;
}

void h(int n, int m, int k) {
    if (n > m) return;
    if (a(n, k)) {
        printf("%d ", n);
        h(n + 1, m, k);
    }
}

int main() {
    int N, M, K;

    printf("Введите N M K: ");
    scanf("%d %d %d", & N, & M, & K);

    if (N < 1 || M < 1 || N > M) {
        printf("Ошибка\n");
        return 1;
    }

    for (int i = N; i <= M; i++) {
        printf("Цепочка: ");
        h(i, M, K);
        printf("\n");
        break;
    }

    return 0;
}
